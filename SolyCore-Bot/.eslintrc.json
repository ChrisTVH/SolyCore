{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [ "jsdoc" ],
    "rules": {
        "jsdoc/no-undefined-types": 1,
        "no-cond-assign": 0,
        "no-unused-vars": [
            "error",
            { "args": "none" }
        ]
    }
}