{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "baseUrl": "./",
        "paths": {
            "@handlers/*": [ "./src/handlers/*" ],
            "@helpers/*": [ "./src/helpers/*" ],
            "@root/*": [ "./*" ],
            "@schemas/*": [ "./src/database/schemas/*" ],
            "@src/*": [ "./src/*" ],
            "@structures/*": [ "./src/structures/*" ]
        }
    },
    "exclude": [ "node_modules", "**/node_modules/*" ]
}